<div class="row register-row">
	<div class="col-sm-4 col-sm-offset-4">
		<div class="panel panel-default" id="signup-panel">
			<div class="panel-heading">
				<h3 class="panel-title">Join Fritter today</h3>
			</div>
			<div class="panel-body">
                <form>
					<div class="login-form-main-message"></div>
					<div class="main-login-form">
						<div class="login-group">
							<div class="form-group">
								<input type="text" class="form-control" id="signup-fullname" placeholder="Full Name">
							</div>
							<div class="form-group">
								<input type="text" class="form-control" id="signup-username" placeholder="Username">
							</div>
							<div class="form-group">
								<input type="password" class="form-control" id="signup-password" placeholder="Password">
							</div>
						</div>
						<button id="signup-submit-btn" class="btn btn-primary">Sign up</button>
					</div>
					<div class="switch-login-signup">
						Already have an account? <a href="#" id="login-page-btn">Log in</a>
					</div>
				</form>
            </div>
		</div>
		<div class="error-msg"></div>
	</div>
</div>